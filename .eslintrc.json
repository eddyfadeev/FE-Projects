{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": { 
        "no-unused-vars": "off",
        "strict":"off",
        "no-plusplus": "off",
        "no-alert": "off",
        "no-restricted-globals": "off",
        "eqeqeq": "off",
        "no-use-before-define": "warn",
        "one-var": "off",
        "no-console": "off",
        "no-trailing-spaces": "off",
        "no-empty": "warn",
        "prefer-const": "off",
        "indent": "off",
        "guard-for-in": "off",
        "no-restricted-syntax": "off",
        "max-len":"off",
        "no-param-reassign": "off",
        "prefer-destructuring": "off",
        "no-prototype-builtins": "off",
        "func-names": "off"
    }
}
